<main class="main">
   <div class="max-w-[788px]">
      <h1 class="mb-8 text-2xl font-semibold tracking-wide sm:text-4xl">
         DARE AURE - Terms of Service (Legal Notice)
      </h1>

      <div class="space-y-9">
         <% for (const term of terms) { %>
         <div>
            <h3 class="mb-2 text-2xl font-bold capitalize">
               <%= term.priority %> . <%= term.title %>
            </h3>
            <p class="font-medium">
               <%= term.description %>.
            </p>
         </div>
         <% } %>
         <div>
         </div>
      </div>
</main>